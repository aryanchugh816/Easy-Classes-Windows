{
  "name": "Easy_Classes",
  "version": "1.1.1",
  "description": "Be_more_efficient_as_my_software_attends_your_classes_for_you",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon --exec electron .",
    "mac": "electron-builder -m",
    "win": "electron-builder -w",
    "build": "electron-builder -mwl",
    "make-cert": "electron-builder create-self-signed-cert -p Arc24_Inc",
    "publish": "electron-builder build -w -p onTagOrDraft"
  },
  "build": {
    "directories": {
      "buildResources": "."
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "mac": {
      "publish": [
        "github"
      ],
      "category": "public.app-category.productivity"
    },
    "win": {
      "publish": [
        "github"
      ],
      "certificateFile": "private/Arc24_Inc.pfx",
      "verifyUpdateCodeSignature": false,
      "publisherName": "Arc24_Inc",
      "extraFiles": [
        "./Utilities",
        "./assets"
      ]
    },
    "linux": {
      "publish": [
        "github"
      ],
      "category": "Network"
    }
  },
  "author": "Arc24_Inc",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.0.0",
    "electron-builder": "^21.2.0",
    "nodemon": "^2.0.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "electron-log": "^4.2.4",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.4",
    "fs": "0.0.1-security",
    "jquery": "^3.5.1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.13",
    "path": "^0.12.7",
    "url": "^0.11.0"
  }
}
